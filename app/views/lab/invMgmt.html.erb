var gmfpData = [];
function opengmfp() {
	$('.gmfp').show();
}

function closegmfp() {
	$('.gmfp').hide();
}

function checkAccount(account) {
	if(account == "" || account == null) {
		$('#account_pass').hide();
		$('#account_error').show();
		return false;
	}
	if(/^[0-9]*$/.test(account)) {
		$('#account_pass').show();
		$('#account_error').hide();
	} else {
		$('#account_pass').hide();
		$('#account_error').show();
	}
	return false;
}
/**
 * 购买发票
 * @returns
 */
function gmfpSubmit() {
	//当没有购买发票时才进行发票购买
	if($('.for_cont_reel').length < 2) {
		var questionId = $('#questionId').val();
		if($('.for_cont_reel').length == 1) {
			questionId = parseInt(questionId) + 1;
		}
		var account = $('#account').val();
		if(account == "" || account == null) {
			$("#account").focus();
			$('#account_pass').hide();
			$('#account_error').show();
			return false;
		}
		var invoiceType = $('#gmfp_type').val();
		if(account != 20) {
			alertInfo("请结合实操助手提供的业务说明进行发票购买！");
			return false;
		}
		//为梦园商贸有限公司（小规模纳税人2月份筹建期）模块中的购买发票，此模块只购买20张普通发票
		if("001001004" == "001001001") {
			//此模块只用购买普通发票，因此length==1说明已经购买了
			if($('.for_cont_reel').length > 0) {
				alertInfo("发票购买业务已经完成。");
				return false;
			}
			//1为普通发票
			if(invoiceType == 1) {
				gmfpData.push({questionId:questionId,answerCode:"questionId[0]",answerDetail:questionId});
				gmfpData.push({questionId:questionId,answerCode:"type[0]",answerDetail:"普通发票"});
				gmfpData.push({questionId:questionId,answerCode:"quota[0]",answerDetail:"9999999.99"});
				gmfpData.push({questionId:questionId,answerCode:"categoryCode[0]",answerDetail:"4200041140"});
				gmfpData.push({questionId:questionId,answerCode:"categoryName[0]",answerDetail:"湖北省普通发票"});
				gmfpData.push({questionId:questionId,answerCode:"startNumber[0]",answerDetail:"0029891"});
				gmfpData.push({questionId:questionId,answerCode:"invoiceAccount[0]",answerDetail:account});
				gmfpData.push({questionId:questionId,answerCode:"getTime[0]",answerDetail:"2019-02-08"});
			} else {
				alertInfo("请结合实操助手提供的业务说明进行发票购买！");
				return false;
			}
			
		} else {
			//用来验证是否买过此类型发票，0为买过普通发票；1为买过专用发票
			var reelType = null;
			if($('.for_cont_reel').length == 1) {
				reelType = $('.for_cont_reel').text();
			}
//			alert(reelType);
			//1为普通发票
			if(invoiceType == 1) {
				if(reelType == 1) {
					alertInfo("已经购买过的发票类型请不要进行重复购买！");
					return false;
				}
				gmfpData.push({questionId:questionId,answerCode:"questionId[0]",answerDetail:questionId});
				gmfpData.push({questionId:questionId,answerCode:"type[0]",answerDetail:"普通发票"});
				gmfpData.push({questionId:questionId,answerCode:"quota[0]",answerDetail:"9999999.99"});
				gmfpData.push({questionId:questionId,answerCode:"categoryCode[0]",answerDetail:"4200041140"});
				gmfpData.push({questionId:questionId,answerCode:"categoryName[0]",answerDetail:"湖北省普通发票"});
				gmfpData.push({questionId:questionId,answerCode:"startNumber[0]",answerDetail:"0029891"});
				gmfpData.push({questionId:questionId,answerCode:"invoiceAccount[0]",answerDetail:account});
				gmfpData.push({questionId:questionId,answerCode:"getTime[0]",answerDetail:"2018-12-03"});
			} else {
				if(reelType == 0) {
					alertInfo("已经购买过的发票类型请不要进行重复购买！");
					return false;
				}
				gmfpData.push({questionId:questionId,answerCode:"questionId[1]",answerDetail:questionId});
				gmfpData.push({questionId:questionId,answerCode:"type[1]",answerDetail:"专用发票"});
				gmfpData.push({questionId:questionId,answerCode:"quota[1]",answerDetail:"9999999.99"});
				gmfpData.push({questionId:questionId,answerCode:"categoryCode[1]",answerDetail:"4200041142"});
				gmfpData.push({questionId:questionId,answerCode:"categoryName[1]",answerDetail:"湖北省专用发票"});
				gmfpData.push({questionId:questionId,answerCode:"startNumber[1]",answerDetail:"0029885"});
				gmfpData.push({questionId:questionId,answerCode:"invoiceAccount[1]",answerDetail:account});
				gmfpData.push({questionId:questionId,answerCode:"getTime[1]",answerDetail:"2018-12-03"});
			}
		}
//		$('.gmfp').hide();
		opacitybg();
		//购买发票
		$.ajax({
//			url:"../question/saveanswernotdelete.json",
			url:"../question/saveMemberAnswersNotCompareIstrue.json",
			data:JSON.stringify(gmfpData),
			contentType:"application/json",
			dataType:"json",
			type:"post",
			success:function(data){
				if(data.success){
					alert("购买成功。");
				}
				location.reload();
			}
		});
	} else {
		alertInfo("发票购买业务已经完成。");
		return false;
	}

}
//红字发票信息表信息选择--radio
function selectRadio(t, n, className) {
	
	var obj = $("." + className).find("input");
	if (t == 0 && n == "a") {
		obj.each(function () {
			if ($(this).attr("name") == "choose12") {
				$(this).removeAttr("disabled");
			} else {
				if ($(this).attr("name") != "choose1") {
					$(this).attr("disabled", "disabled");
					$(this).removeAttr("checked");
				}
			}
		});
	}
	if (t == 1 && n == "a") {
		obj.each(function () {
			if ($(this).attr("name") == "choose21") {
				$(this).removeAttr("disabled");
			} else {
				if ($(this).attr("name") != "choose1") {
					$(this).attr("disabled", "disabled");
					$(this).removeAttr("checked");
				}
			}
		});
	}
	if (t == 1 && n == "b") {
		obj.each(function () {
			if ($(this).attr("name") == "choose121") {
				$(this).removeAttr("disabled");
			} else {
				if ($(this).attr("name") != "choose1" && $(this).attr("name") != "choose12") {
					$(this).attr("disabled", "disabled");
					$(this).removeAttr("checked");
				}
			}
		});
	}
	if (t == 0 && n == "b") {
		obj.each(function () {
			if ($(this).attr("name") != "choose1" && $(this).attr("name") != "choose12") {
				$(this).attr("disabled", "disabled");
				$(this).removeAttr("checked");
			}
		});
	}
}

