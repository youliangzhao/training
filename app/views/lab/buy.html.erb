/**
 * 查询税收分类编码列表
 */
function selectByAjax() {
	var $productClassTable = $(".proclass .choi_scroll .choi_cont table tbody");
    $productClassTable.empty();//清空内容  
    var tbody = "";
	var pageInfo = getPageInfo();
	var searchInfo = $(".proclass_info_search").val();
	var data = pageInfo;
	var proclassCode = $(".proclass_code").val();
	//放入搜索内容
	if(searchInfo != "") {
		data += "&searchInfo="+searchInfo;
	}
	//放入上级编码
	if(proclassCode != "") {
		data += "&code="+proclassCode;
	}
	data = encodeURI(data);
	$.ajax({
		type : "POST",
		url : "http://lab1.ruby.com:8081/product/findProductClassList.json",
		data : data,
		success : function(msg) {
			$.each(msg.list,function(infoIndex,info){  
	            var collect = info["collect"];
	            var collectValue = "";
	            if(collect == "Y") {
	            	collectValue = "是";
	            } else {
	            	collectValue = "否";
	            }
				var tr = "<tr><td class='ta_left'></td><td id='code' style='width: 4%;'>" + info["code"] + "</td>" +
	            		"<td id='mergeCode' style='width: 6%;'>" + info["mergeCode"] + "</td>" +
	            		"<td id='name' style='width: 9%;'><p style='width: 135px;' title='" + info["name"] + "'>" + info["name"] + "</p></td>" +
	            		"<td id='abbreviation' style='width: 9%;'><p style='width: 135px;' title='" + info["abbreviation"] + "'>" + info["abbreviation"] + "</p></td>" +
	            		"<td id='explain' style='width: 11%;'><p style='width: 200px;' title='" + info["explain"] + "'>" + info["explain"] + "</p></td>" +
	            		"<td id='taxRate' style='width: 4%;'>" + info["taxRate"] + "</td>" +
	            		"<td id='zzsSpecialManage' style='width: 5%;'>" + info["zzsSpecialManage"] + "</td>" +
	            		"<td id='zzsPolicyGist' style='width: 5%;'><p style='width: 85px;' title='" + info["zzsPolicyGist"] + "'>" + info["zzsPolicyGist"] + "</p></td>" +
	            		"<td id='zzsSpecialCode' style='width: 5%;'><p style='width: 85px;' title='" + info["zzsSpecialCode"] + "'>" + info["zzsSpecialCode"] + "</p></td>" +
	            		"<td id='xfsManage' style='width: 5%;'>" + info["xfsManage"] + "</td>" +
	            		"<td id='xfsPolicyGist' style='width: 5%;'><p style='width: 85px;' title='" + info["xfsPolicyGist"] + "'>" + info["xfsPolicyGist"] + "</p></td>" +
	            		"<td id='xfsSpecialCode' style='width: 5%;'><p style='width: 85px;' title='" + info["xfsSpecialCode"] + "'>" + info["xfsSpecialCode"] + "</p></td>" +
	            		"<td id='keyword' style='width: 11%;'><p style='width: 175px;' title='" + info["keyword"] + "'>" + info["keyword"] + "</p></td>" +
	            		"<td id='statistics' style='width: 5%;'><p style='width: 110px;' title='" + info["statistics"] + "'>" + info["statistics"] + "</p></td>" +
	            		"<td style='width: 4%;'><input type='hidden' id='isLowest' value='" + info["isLowest"] + "'/></td>" +
	            		"<td style='width: 4%;'></td>" +
	            		"<td style='width: 4%;'></td>" +
	            		"<td id='collect' style='width: 3%;'>" + collectValue + "</td>" +
	            		"<td id='isHide' style='width: 4%;'>否</td></tr>";
	            tbody += tr;
	        });
	        $productClassTable.append(tbody);//显示处理后的数据
	        //传入分页数据
	        //第几页
	        var pageIndex = msg.pageInfo.pageIndex;
	        //每页几条数据
	        var pageSize = msg.pageInfo.pageSize;
	        //当前条数
	        var pageRowNum = (pageIndex - 1) * pageSize + 1;
	        //总数据条数
	        var totalRowNum = msg.pageInfo.totalRowNum;
	        //总页数
	        var totalIndex = Math.ceil(totalRowNum/pageSize);
	        //当为第一页时，屏蔽前一页按钮
	        if(pageIndex == 1) {
	        	$(".pageInfo .prev").attr("disabled",true);
	        } else {
	        	$(".pageInfo .prev").attr("disabled",false);
	        }
	        //当当前页为最后一页时屏蔽后一页按钮
	        if(pageIndex == totalIndex) {
	        	$(".pageInfo .next").attr("disabled",true);
	        } else {
	        	$(".pageInfo .next").attr("disabled",false);
	        }
	        $(".pageIndex").text(pageIndex);
	        $("#pageSize").text(pageSize);
	        $("#pageRowNum").text(pageRowNum);
	        $("#totalRowNum").text(msg.pageInfo.totalRowNum);
	        $("#totalIndex").text(totalIndex);
	        //双击选中时进行填值
	    	$(".proclass .choi_scroll .choi_cont").find("tr:not(:first)").dblclick(function(){
	    		//当为最下级时才能被选中，不然进行提示
	    		if($(this).find("#isLowest").val() == "1") {
	    			var form=$(".commodity").find(".adding,.revised").find(":visible");
	    			form.find("input#taxClassCode").val($(this).find("#code").text());
	    			form.find("input#taxClassName").val($(this).find("#name").text());
	    			form.find("input#taxclassAbbreviation").val($(this).find("#abbreviation").text());
	    			$(".proclass").hide();
	    		} else {
	    			alertInfo("此编码不为最下级编码，请选择最下级。");
	    		}
	    	});
		}
	});
}

